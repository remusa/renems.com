{
  "name": "gatsby-starter-default",
  "private": true,
  "description": "A simple starter to get up and developing quickly with Gatsby",
  "baseUrl": "https://renems.com/",
  "version": "0.1.0",
  "author": "RMS <contact@renems.com>",
  "scripts": {
    "start": "npm run develop",
    "dev": "npm-run-all --parallel dev:*",
    "dev2:playground": "cross-env GATSBY_GRAPHQL_IDE=playground npm-run-all --parallel dev:*",
    "dev:gatsby": "gatsby develop",
    "dev2:functions": "dotenv -e ./netlify/.env netlify-lambda serve netlify/functions",
    "develop": "gatsby develop",
    "clean": "gatsby clean",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "setup": "npm install && npm run validate",
    "convert": "synp --source-file yarn.lock -f",
    "check-types": "tsc",
    "lint": "eslint --ignore-path .gitignore --ext .js,.jsx,.ts,.tsx .",
    "lint:fix": "npm run lint -- --fix",
    "prettier": "prettier --ignore-path .gitignore \"**/*.+(js|jsx|ts|tsx|json)\"",
    "format": "npm run prettier -- --write",
    "check-format": "npm run prettier -- --list-different",
    "analyze": "cross-env-shell ANALYZE_BUNDLE_SIZE=true & npm run build",
    "deploy": "npm run deploy:netlify",
    "deploy:netlify": "netlify deploy --dir=./public -p -m \"$(git log -1 --pretty=%B)\"",
    "validate": "npm-run-all --parallel check-types check-format lint build",
    "test": "is-ci \"test:coverage\" \"test:watch\"",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "test:e2e": "start-server-and-test start http://localhost:5020 cy:run",
    "test:debug": "node --inspect-brk ./node_modules/jest/bin/jest.js --runInBand --watch",
    "cy": "is-ci \"test:e2e\" \"cy:open\"",
    "cy:open": "cypress open",
    "cy:run": "cross-env CYPRESS_BASE_URL=http://localhost:5020 cypress run",
    "upgrade-interactive": "npm-check --update"
  },
  "dependencies": {
    "@emotion/core": "^10.0.28",
    "@emotion/styled": "^10.0.27",
    "@mdx-js/mdx": "^1.6.7",
    "@mdx-js/react": "^1.6.7",
    "aos": "^3.0.0-beta.6",
    "gatsby": "^4.24.5",
    "gatsby-cli": "^4.24.0",
    "gatsby-image": "^2.4.13",
    "gatsby-plugin-emotion": "^4.3.10",
    "gatsby-plugin-feed": "^2.5.11",
    "gatsby-plugin-manifest": "^4.24.0",
    "gatsby-plugin-mdx": "^4.3.0",
    "gatsby-plugin-netlify": "^2.3.11",
    "gatsby-plugin-offline": "^3.2.18",
    "gatsby-plugin-react-helmet": "^3.3.10",
    "gatsby-plugin-sharp": "^4.24.0",
    "gatsby-plugin-sitemap": "^2.4.11",
    "gatsby-plugin-transition-link": "^1.17.7",
    "gatsby-plugin-webpack-bundle-analyzer": "^1.0.5",
    "gatsby-source-filesystem": "^2.3.19",
    "gatsby-transformer-sharp": "^4.24.0",
    "gsap": "^3.4.0",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-helmet": "^6.0.0",
    "react-icons": "^3.8.0"
  },
  "devDependencies": {
    "@babel/core": "^7.10.5",
    "@testing-library/jest-dom": "^5.11.1",
    "@testing-library/react": "^10.4.7",
    "@testing-library/react-hooks": "^3.3.0",
    "@testing-library/user-event": "^12.0.11",
    "@types/jest": "^26.0.4",
    "@types/node": "^14.0.23",
    "@types/react": "^16.9.43",
    "@types/react-dom": "^16.9.8",
    "@typescript-eslint/eslint-plugin": "^3.6.1",
    "@typescript-eslint/parser": "^3.6.1",
    "babel-eslint": "10.x",
    "babel-jest": "^26.1.0",
    "babel-plugin-emotion": "^10.0.33",
    "babel-preset-gatsby": "^0.5.2",
    "cross-env": "^7.0.2",
    "eslint": "^7.4.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-config-react-app": "^5.2.1",
    "eslint-plugin-flowtype": "^5.2.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest-dom": "^3.1.4",
    "eslint-plugin-jsx-a11y": "6.x",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.20.3",
    "eslint-plugin-react-hooks": "^4.0.8",
    "eslint-plugin-testing-library": "^3.3.1",
    "graphql-tools": "^6.2.6",
    "husky": "^4.3.8",
    "identity-obj-proxy": "^3.0.0",
    "is-ci-cli": "^2.1.2",
    "jest": "^26.1.0",
    "jest-in-case": "^1.0.2",
    "jest-transform-graphql": "^2.1.0",
    "lint-staged": "^10.2.11",
    "npm-check": "^6.0.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.1.2",
    "typescript": "^3.9.6"
  },
  "keywords": [
    "portfolio",
    "gatsby",
    "react"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-default"
  },
  "bugs": {
    "url": "https://github.com/remusa/renems.com/issues"
  },
  "browserslist": [
    "last 1 version",
    "> 1%",
    "maintained node versions",
    "not dead"
  ]
}
