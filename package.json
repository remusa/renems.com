{
	"name": "renems.com",
	"version": "3.0.0",
	"type": "module",
	"scripts": {
		"start": "astro dev",
		"build": "astro check && astro build && jampack ./dist",
		"preview": "astro preview",
		"sync": "astro sync",
		"astro": "astro",
		"check": "astro check",
		"format": "nlx @biomejs/biome format ./src --write",
		"lint": "nlx @biomejs/biome lint ./src ",
		"fix": "nlx @biomejs/biome check --write --unsafe ./src",
		"ci": "biome format --changed",
		"knip": "knip",
		"cz": "cz",
		"postinstall": "lefthook install",
		"clean": "nlx rimraf node_modules/ .astro/ dist/",
		"deps": "nlx taze -Iwi",
		"deps:upgrade": "npx @astrojs/upgrade"
	},
	"dependencies": {
		"@astrojs/check": "^0.9.4",
		"@astrojs/mdx": "^4.0.0",
		"@astrojs/rss": "^4.0.9",
		"@astrojs/svelte": "^7.0.0",
		"@fontsource-variable/assistant": "^5.1.0",
		"@fontsource/inter": "^5.1.0",
		"@resvg/resvg-js": "^2.6.2",
		"@unpic/astro": "^0.1.0",
		"astro": "^5.0.0",
		"fuse.js": "^7.0.0",
		"github-slugger": "^2.0.0",
		"mdast-util-to-string": "^4.0.0",
		"reading-time": "^1.5.0",
		"remark-collapse": "^0.1.2",
		"remark-toc": "^9.0.0",
		"satori": "^0.12.0",
		"tailwindcss": "^3.4.15"
	},
	"devDependencies": {
		"@astrojs/react": "^4.0.0",
		"@astrojs/sitemap": "^3.2.1",
		"@astrojs/tailwind": "^5.1.2",
		"@biomejs/biome": "^1.9.4",
		"@commitlint/cli": "^19.5.0",
		"@commitlint/config-conventional": "^19.5.0",
		"@divriots/jampack": "^0.30.0",
		"@tailwindcss/typography": "^0.5.15",
		"@types/node": "^22.9.0",
		"@types/react": "^19.0.0",
		"commitizen": "^4.3.1",
		"cz-conventional-changelog": "^3.3.0",
		"knip": "^5.37.1",
		"lefthook": "^1.8.2",
		"react": "^19.0.0",
		"react-dom": "^19.0.0",
		"svelte": "5.9.0",
		"taze": "^0.18.0",
		"typescript": "^5.6.3"
	},
	"config": {
		"commitizen": {
			"path": "./node_modules/cz-conventional-changelog"
		}
	},
	"engines": {
		"node": ">=18"
	},
	"engineStrict": true,
	"pnpm": {
		"overrides": {
			"dset@<3.1.4": ">=3.1.4",
			"fast-xml-parser@<4.4.1": ">=4.4.1",
			"ip@=2.0.0": ">=2.0.1",
			"micromatch@<4.0.8": ">=4.0.8",
			"path-to-regexp@>=4.0.0 <6.3.0": ">=6.3.0",
			"rollup@>=4.0.0 <4.22.4": ">=4.22.4",
			"tar@<6.2.1": ">=7.4.0",
			"undici@<=5.28.2": ">=6.19.2",
			"undici@>=6.0.0 <6.11.1": ">=6.11.1",
			"vite@>=5.1.0 <=5.1.6": ">=5.1.7",
			"vite@>=5.4.0 <=5.4.5": ">=5.4.6",
			"vite@>=5.4.0 <5.4.6": ">=5.4.6"
		}
	}
}
