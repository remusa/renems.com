{
  "name": "gatsby-starter-default",
  "private": true,
  "description": "A simple starter to get up and developing quickly with Gatsby",
  "baseUrl": "https://renems.com/",
  "version": "0.1.0",
  "author": "RMS <contact@renems.com>",
  "scripts": {
    "start": "npm run develop",
    "dev": "npm-run-all --parallel dev:*",
    "dev2:playground": "cross-env GATSBY_GRAPHQL_IDE=playground npm-run-all --parallel dev:*",
    "dev:gatsby": "gatsby develop",
    "dev2:functions": "dotenv -e ./netlify/.env netlify-lambda serve netlify/functions",
    "develop": "gatsby develop",
    "clean": "gatsby clean",
    "clean:local": "npx rimraf -f node_modules/",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "setup": "npm run clean:local && npm install && npm run validate",
    "convert": "synp --source-file yarn.lock -f",
    "check-types": "tsc",
    "lint": "eslint --ignore-path .gitignore --ext .js,.jsx,.ts,.tsx .",
    "lint:fix": "npm run lint -- --fix",
    "prettier": "prettier --ignore-path .gitignore \"**/*.+(js|jsx|ts|tsx|json)\"",
    "format": "npm run prettier -- --write",
    "check-format": "npm run prettier -- --list-different",
    "analyze": "cross-env-shell ANALYZE_BUNDLE_SIZE=true & npm run build",
    "deploy": "npm run deploy:netlify",
    "deploy:netlify": "netlify deploy --dir=./public -p -m \"$(git log -1 --pretty=%B)\"",
    "validate": "npm-run-all --parallel check-types check-format lint build",
    "test": "is-ci \"test:coverage\" \"test:watch\"",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "test:e2e": "start-server-and-test start http://localhost:5020 cy:run",
    "test:debug": "node --inspect-brk ./node_modules/jest/bin/jest.js --runInBand --watch",
    "cy": "is-ci \"test:e2e\" \"cy:open\"",
    "cy:open": "cypress open",
    "cy:run": "cross-env CYPRESS_BASE_URL=http://localhost:5020 cypress run",
    "deps": "npx taze -Iwi"
  },
  "dependencies": {
    "@emotion/core": "^10.3.1",
    "@emotion/styled": "^10.3.0",
    "@mdx-js/mdx": "^1.6.22",
    "@mdx-js/react": "^1.6.22",
    "aos": "^3.0.0-beta.6",
    "gatsby": "^2.32.13",
    "gatsby-cli": "^2.19.3",
    "gatsby-image": "^2.11.0",
    "gatsby-plugin-emotion": "^4.5.0",
    "gatsby-plugin-feed": "^2.13.1",
    "gatsby-plugin-manifest": "^2.12.1",
    "gatsby-plugin-mdx": "^1.10.1",
    "gatsby-plugin-netlify": "^2.11.1",
    "gatsby-plugin-offline": "^3.10.2",
    "gatsby-plugin-react-helmet": "^3.10.0",
    "gatsby-plugin-sharp": "^2.14.4",
    "gatsby-plugin-sitemap": "^2.12.0",
    "gatsby-plugin-transition-link": "^1.20.5",
    "gatsby-plugin-webpack-bundle-analyzer": "^1.0.5",
    "gatsby-source-filesystem": "^2.11.1",
    "gatsby-transformer-sharp": "^2.12.1",
    "gsap": "^3.11.4",
    "prop-types": "^15.8.1",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-helmet": "^6.1.0",
    "react-icons": "^3.11.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^10.4.9",
    "@testing-library/react-hooks": "^3.7.0",
    "@testing-library/user-event": "^12.8.3",
    "@types/jest": "^26.0.24",
    "@types/node": "^14.18.36",
    "@types/react": "^16.14.34",
    "@types/react-dom": "^16.9.17",
    "@typescript-eslint/eslint-plugin": "^3.10.1",
    "@typescript-eslint/parser": "^3.10.1",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.3",
    "babel-plugin-emotion": "^10.2.2",
    "babel-preset-gatsby": "^0.5.16",
    "cross-env": "^7.0.3",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-config-react-app": "^5.2.1",
    "eslint-plugin-flowtype": "^5.10.0",
    "eslint-plugin-import": "^2.27.4",
    "eslint-plugin-jest-dom": "^3.9.4",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^3.4.1",
    "eslint-plugin-react": "^7.32.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-testing-library": "^3.10.2",
    "graphql-tools": "^6.2.6",
    "husky": "^4.3.8",
    "identity-obj-proxy": "^3.0.0",
    "is-ci-cli": "^2.2.0",
    "jest": "^26.6.3",
    "jest-in-case": "^1.0.2",
    "jest-transform-graphql": "^2.1.0",
    "lint-staged": "^10.5.4",
    "npm-check": "^5.9.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.3",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.5.6",
    "typescript": "^3.9.10"
  },
  "keywords": [
    "portfolio",
    "gatsby",
    "react"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-default"
  },
  "bugs": {
    "url": "https://github.com/remusa/renems.com/issues"
  },
  "browserslist": [
    "last 1 version",
    "> 1%",
    "maintained node versions",
    "not dead"
  ],
  "volta": {
    "node": "18.13.0"
  }
}
