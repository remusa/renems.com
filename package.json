{
  "name": "gatsby-starter-default",
  "private": true,
  "description": "A simple starter to get up and developing quickly with Gatsby",
  "baseUrl": "https://renems.com/",
  "version": "0.1.0",
  "author": "RMS <contact@renems.com>",
  "type": "module",
  "scripts": {
    "dev": "svelte-kit dev",
    "build": "svelte-kit build",
    "package": "svelte-kit package",
    "preview": "svelte-kit preview",
    "prepare": "svelte-kit sync",
    "test": "echo \"Testing!\"",
    "test:e2e": "playwright test",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
    "lint:fix": "eslint --ignore-path .gitignore . ",
    "format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
    "fix": "run-p format lint:fix",
    "validate": "run-p check lint build",
    "upgrade-interactive": "pnpm dlx  npm-check --update",
    "commitlint": "pnpm dlx  --no -- commitlint --edit \"$1\"",
    "depcheck": "pnpm dlx depcheck",
    "deps:check": "pnpm dlx npm-check-updates",
    "deps:check:dev": "pnpm deps:check -- --dep dev",
    "deps:check:prod": "pnpm deps:check -- --dep prod",
    "deps:update:patch": "pnpm deps:check -- -u -t patch",
    "deps:update:minor": "pnpm deps:check -- -u -t minor",
    "deps:update:all": "pnpm deps:check -- -u"
  },
  "dependencies": {
    "@fontsource/fira-mono": "^4.5.7",
    "@lukeed/uuid": "^2.0.0",
    "clsx": "^1.1.1",
    "cookie": "^0.5.0",
    "date-fns": "^2.28.0",
    "focus-visible": "^5.2.0",
    "heroicons-svelte": "^0.0.1",
    "reading-time-estimator": "^1.5.0"
  },
  "devDependencies": {
    "@arkweid/lefthook": "^0.7.7",
    "@commitlint/cli": "^16.2.3",
    "@commitlint/config-conventional": "^16.2.1",
    "@playwright/test": "^1.21.0",
    "@sveltejs/adapter-auto": "next",
    "@sveltejs/adapter-static": "^1.0.0-next.29",
    "@sveltejs/kit": "^1.0.0-next.314",
    "@tailwindcss/typography": "^0.5.2",
    "@types/cookie": "^0.5.0",
    "@typescript-eslint/eslint-plugin": "^5.19.0",
    "@typescript-eslint/parser": "^5.19.0",
    "autoprefixer": "^10.4.4",
    "cross-env": "^7.0.3",
    "cssnano": "^5.1.7",
    "eslint": "^8.13.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-svelte3": "^3.4.1",
    "hastscript": "^7.0.2",
    "mdsvex": "^0.10.5",
    "mdsvex-relative-images": "^1.0.2",
    "node-html-parser": "^5.3.3",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.12",
    "postcss-focus-visible": "^6.0.4",
    "postcss-load-config": "^3.1.4",
    "prettier": "~2.6.2",
    "prettier-plugin-svelte": "^2.7.0",
    "reading-time": "^1.5.0",
    "rehype-autolink-headings": "^6.1.1",
    "rehype-parse": "^8.0.4",
    "rehype-slug": "^5.0.1",
    "rehype-stringify": "^9.0.3",
    "remark-abbr": "^1.4.1",
    "remark-reading-time": "^2.0.1",
    "sass": "^1.50.0",
    "svelte": "^3.47.0",
    "svelte-check": "^2.7.0",
    "svelte-preprocess": "^4.10.6",
    "svelte-toc": "^0.2.9",
    "tailwindcss": "^3.0.24",
    "tailwindcss-font-inter": "^3.0.1",
    "tslib": "^2.3.1",
    "typescript": "^4.6.3",
    "unified": "^10.1.2",
    "unist-util-visit": "^4.1.0"
  },
  "keywords": [
    "portfolio",
    "blog",
    "sveltekit",
    "svelte"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/remusa/renems.com"
  },
  "bugs": {
    "url": "https://github.com/remusa/renems.com/issues"
  },
  "browserslist": [
    "last 1 version",
    "> 1%",
    "maintained node versions",
    "not dead"
  ],
  "engineStrict": true,
  "engines": {
    "node": ">=14.x",
    "pnpm": ">=7.x"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  }
}
